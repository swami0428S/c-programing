Series-02

#include <stdio.h>
#include <stdbool.h>
bool isPrime(int num) {
    if (num < 2) return false;
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0)
            return false;
    }
    return true;
}

int main() {
    int n;
    scanf("%d", &n);

    int count = 0;
    int num = 101;

    while (count < n) {
        if (isPrime(num)) {
            printf("%d", num);
            count++;
            if (count < n) printf(" ");
        }
        num++;
    }
    printf("\n");
    return 0;
}
