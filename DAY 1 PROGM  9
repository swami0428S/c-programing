

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

    int main() {
    float a=67.98;
    scanf("%f",&a);
    printf("%.06f\n",a);
    printf("%.04f\n",a);
    printf("%.02f\n",a);
    printf("%d",(int)a+1);
    return 0;

}
