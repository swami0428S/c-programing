

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n;
    scanf("%d", &n);  

    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); 
    }

    int min = INT_MAX;
    int second_min = INT_MAX;

    for(int i = 0; i < n; i++) {
        if(arr[i] < min) {
            second_min = min;
            min = arr[i];
        } else if(arr[i] > min && arr[i] < second_min) {
            second_min = arr[i];
        }
    }

    if(second_min == INT_MAX) {
        printf("No second minimum element\n");
    } else {
        printf("%d\n", second_min);
    }

    return 0;
}
