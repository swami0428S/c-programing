Find K Largest Elements

#include <stdio.h>
#include <stdlib.h>
int compare(const void *a, const void *b) {
    int x = *(int*)a;
    int y = *(int*)b;
    return x - y;
}

int main() {
    int n, k;
    scanf("%d", &n);

    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    scanf("%d", &k);
    qsort(arr, n, sizeof(int), compare);
    printf("The elements are in the order:");
    for(int i = 0; i < n; i++) {
        printf(" %d", arr[i]);
    }
    printf("\n");
    printf("The Kth value is %d and Largest elements are", k);
    for(int i = n - 1; i >= n - k; i--) {
        printf(" %d", arr[i]);
    }
    printf("\n");

    return 0;
}
