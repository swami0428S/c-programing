Alice in wonderland

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int number,digit1,digit2,sum;
    scanf("%d",&number);
    digit1=number/10;
    digit2=number%10;
    sum=digit1+digit2;
    printf("Bird said:%d\n",number);
    printf("Alice must go in path-%d\n",sum);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
